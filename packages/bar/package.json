{
    "name": "@devinit-charts/bar",
    "version": "1.0.0",
    "description": "Modular D3 bar charts for Devinit DataHub",
    "main": "lib/index.js",
    "scripts": {
      "build": "tsc",
      "lint": "tslint -c ../../tslint.json 'src/**/*.ts' ",
      "patch": "release-it -i patch -n",
      "feature": "release-it -i minor -n",
      "breaking": "release-it -i major -n"
    },
    "dependencies": {
      "@devinit/prelude": "^1.1.1",
      "@devinit-charts/core": "1.0.0",
      "d3": "5.16.0",
      "lodash": "^4.17.5",
      "plottable": "3.8.2",
      "tooltip.js": "^1.2.0"
    },
    "directories": {
      "lib": "./lib",
      "src": "./src",
      "test": "./test"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/devinit/charts.git"
    },
    "keywords": [
      "charts",
      "line-charts",
      "plottable",
      "bar",
      "d3"
    ],
    "git-pre-hooks": {
      "pre-push": [
        "npm run compile",
        "npm run lint"
      ]
    },
    "jest": {
      "transform": {
        ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
      },
      "coverageDirectory": "./coverage/",
      "collectCoverage": true,
      "testEnvironment": "node",
      "testRegex": ".*\\.test\\.ts$",
      "moduleFileExtensions": [
        "ts",
        "js",
        "json"
      ]
    },
    "author": "Alex Miller",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/devinit/charts/issues"
    },
    "homepage": "https://github.com/devinit/charts#readme"
  }
  