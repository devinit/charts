{
    "name": "@devinit-charts/core",
    "version": "1.0.0",
    "description": "Core package for devinit charts",
    "main": "lib/index.js",
    "scripts": {
      "test": "jest",
      "test:dev": "jest -w",
      "build": "tsc",
      "lint": "tslint -c ../../tslint.json 'src/**/*.ts' ",
      "patch": "release-it -i patch -n",
      "feature": "release-it -i minor -n",
      "breaking": "release-it -i major -n"
    },
    "dependencies": {
      "@devinit/prelude": "^1.1.1",
      "d3": "4.13.0",
      "lodash": "^4.17.5",
      "plottable": "3.8.1"
    },
    "directories": {
      "dist": "./dist",
      "lib": "./lib",
      "src": "./src",
      "test": "./test"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/devinit/charts.git"
    },
    "keywords": [
      "charts",
      "tree-charts",
      "plottable",
      "trees",
      "d3"
    ],
    "git-pre-hooks": {
      "pre-push": [
        "npm run build",
        "npm run lint"
      ]
    },
    "jest": {
      "transform": {
        ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
      },
      "coverageDirectory": "./coverage/",
      "collectCoverage": true,
      "testEnvironment": "node",
      "testRegex": ".*\\.test\\.ts$",
      "moduleFileExtensions": [
        "ts",
        "js",
        "json"
      ]
    },
    "author": "Alex Miller",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/devinit/charts/issues"
    },
    "homepage": "https://github.com/devinit/charts#readme"
  }
  