{
  "name": "@devinit/charts",
  "version": "1.6.53",
  "description": "Modular D3 charts for DevInit DataHub",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest -w",
    "start": "webpack-dev-server",
    "compile": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'schema/**/*.ts' ",
    "build": "webpack  --env=dev --profile --colors",
    "build:production": "WEBPACK_ENV=production webpack --env=prod --profile --colors",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n",
    "schema": "mkdir -p dist && jsdoc --template schema --destination dist/di-charts.schema.json lib/charts lib/factories",
    "schema:dev": "npm run compile && npm run schema"
  },
  "dependencies": {
    "@devinit/prelude": "^1.1.1",
    "chai-stats": "^0.3.0",
    "d3": "4.10.2",
    "d3-drag": "^1.1.0",
    "lodash": "^4.17.5",
    "object-hash": "^1.1.8",
    "plottable": "3.8.0",
    "tooltip.js": "^1.2.0"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/d3-drag": "^1.2.0",
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.105",
    "@types/object-hash": "^1.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "git-pre-hooks": "^1.2.0",
    "happypack": "^4.0.1",
    "jest": "^22.4.2",
    "jest-cli": "^23.0.0-alpha.0",
    "jsdoc": "^3.4.3",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "node-dev": "^3.1.3",
    "progress-bar-webpack-plugin": "^1.11.0",
    "release-it": "^7.2.1",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "4.1.1",
    "webpack-cli": "^2.0.13",
    "webpack-serve": "^0.2.0"
  },
  "directories": {
    "dist": "./dist",
    "lib": "./lib",
    "src": "./src",
    "test": "./test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/charts.git"
  },
  "keywords": [
    "charts",
    "d3"
  ],
  "git-pre-hooks": {
    "pre-push": [
      "npm run compile",
      "npm run lint"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "author": "Alex Miller",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devinit/charts/issues"
  },
  "homepage": "https://github.com/devinit/charts#readme"
}
